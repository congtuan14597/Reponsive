{
  "name": "no-skeleton.docpad",
  "version": "0.1.0",
  "description": "New DocPad project without using a skeleton",
  "engines" : {
    "node": "6",
    "npm": "3"
  },
  "dependencies": {
    "docpad": "~6.82.5",
    "docpad-plugin-ghpages": "~2.7.0",
    "docpad-plugin-jade": "~2.10.0",
    "docpad-plugin-livereload": "~2.10.0",
    "docpad-plugin-sass": "~2.9.0"
  },
  "main": "node_modules/.bin/docpad-server",
  "scripts": {
    "start": "node_modules/.bin/docpad-server",
    "test": "docpad generate --debug --silent --env static",
     "info": "docpad info --silent"
  }
}
